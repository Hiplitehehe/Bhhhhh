{
  "version": 2,
  "builds": [
    { "src": "api/generate_code.py", "use": "@vercel/python" },
    { "src": "api/generate_code.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/generate_code", "dest": "api/generate_code.js" },
    {
      "src": "/auth",
      "dest": "/api/index.js"
    },
    {
      "src": "/oauth2callback",
      "dest": "/api/index.js"
    },
    {
      "src": "/refresh",
      "dest": "/api/Hjkkk.py"
    }
  ]
}
